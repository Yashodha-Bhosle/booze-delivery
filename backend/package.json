{
  "name": "beerncheer-backend",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "description": "Backend for Beer'n'Cheer",
  "scripts": {
    "server": "nodemon --watch . --ext js,json --exec node server.js",
    "dev": "nodemon",
    "reset": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && npm install",
    "clean": "rimraf node_modules package-lock.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.1",
    "rimraf": "^5.0.5",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "overrides": {
    "glob": "^10.3.10",
    "gauge": "^5.0.1"
  }
}
